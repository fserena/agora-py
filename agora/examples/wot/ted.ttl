@prefix core: <http://agora.org#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix wot: <http://www.wot.org#> .


_:ted1 a core:TED ;
    core:describesEcosystem _:eco .

_:eco a core:Ecosystem ;
    core:includes _:service, <http://es.dbpedia.org/resource/Madrid>,
                             <http://es.dbpedia.org/resource/A%C3%B1ora> .

<http://es.dbpedia.org/resource/Madrid> a <http://schema.org/Place> .
<http://es.dbpedia.org/resource/A%C3%B1ora> a <http://schema.org/Place> .

_:service a wot:Service .

_:td_service a core:ThingDescription ;
    core:identifier "service" ;
    core:describes _:service ;
    core:hasMappingRelation [
        a core:MappingRelation ;
        core:forEndpoint [
            a wot:Endpoint ;
            wot:href "http://localhost:5005/things"^^xsd:anyURI ;
            wot:mediaType "application/json"
        ] ;
        core:hasMapping [
            a core:Mapping ;
            core:key "exposesWebThing" ;
            core:predicate wot:exposesWebThing ;
            core:valuesTransformedBy _:td_stars
        ]
    ] .

_:td_stars a core:ThingDescription ;
    core:identifier "stars" ;
    core:describes [ a wot:Thing ] ;
    core:hasMappingRelation [
        a core:MappingRelation ;
        core:forEndpoint [
            a wot:Endpoint ;
            wot:href "$item" ;
            wot:mediaType "application/json"
        ] ;
        core:hasMapping _:id_map, _:creation_map, _:property_map
    ] .

_:id_map a core:Mapping ;
    core:key "identifier" ;
    core:predicate wot:identifier .

_:creation_map a core:Mapping ;
    core:key "creationDate" ;
    core:predicate wot:creationDate .

_:property_map a core:Mapping ;
    core:key "hasWebThingProperty" ;
    core:predicate wot:offersInteraction ;
    core:valuesTransformedBy _:td_property .

_:td_property a core:ThingDescription ;
    core:identifier "property" ;
    core:describes [ a wot:Property ] ;
    core:hasMappingRelation [
        a core:MappingRelation ;
        core:forEndpoint [
            a wot:Endpoint ;
            wot:href "$item" ;
            wot:mediaType "application/json"
        ] ;
        core:hasMapping _:label_map, _:ts_map, _:latest_map, _:value_map
    ] .

_:label_map a core:Mapping ;
    core:key "label" ;
    core:predicate rdfs:label .

_:ts_map a core:Mapping ;
    core:key "entryTimeStamp" ;
    core:predicate wot:entryTimeStamp .

_:value_map a core:Mapping ;
    core:key "value" ;
    core:predicate wot:value .

_:latest_map a core:Mapping ;
    core:key "hasLatestEntry" ;
    core:predicate wot:hasLatestEntry .
