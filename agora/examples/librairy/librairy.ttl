@prefix librairy: <http://drinventor.dia.fi.upm.es/vocabulary#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://drinventor.dia.fi.upm.es/vocabulary> rdf:type owl:Ontology .

librairy:DocumentService a owl:Class .

librairy:Entity a owl:Class ;
        rdfs:subClassOf [
                            rdf:type owl:Restriction ;
                            owl:onProperty librairy:identifier ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                        ],
                        [
                            rdf:type owl:Restriction ;
                            owl:onProperty librairy:creationTime ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                        ] .

librairy:Document rdfs:subClassOf librairy:Entity,
                        [
                            rdf:type owl:Restriction ;
                            owl:onProperty librairy:title ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                        ] .

librairy:identifier a owl:DatatypeProperty ;
    rdfs:range xsd:string .

librairy:title a owl:DatatypeProperty ;
    rdfs:range xsd:string .

librairy:exposesDocument a owl:ObjectProperty ;
    rdfs:domain librairy:DocumentService ;
    rdfs:range librairy:Document .

librairy:creationTime a owl:DatatypeProperty ;
    rdfs:domain librairy:Entity ;
    rdfs:range xsd:dateTime .
